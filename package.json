    "babel-standalone": "^6.26.0",
    "babylon": "^6.18.0",
    "bin-to-file": "^1.0.4",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "commonmark": "^0.28.1",
    "css-loader": "0.28.4",
    "date-fns": "^1.28.5",
    "dotenv": "4.0.0",
    "emmet-codemirror": "^1.2.5",
    "eslint-loader": "1.7.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "file-saver": "^1.3.3",
    "fs-extra": "3.0.1",
    "fuse.js": "^3.0.5",
    "haikunator": "^2.0.0",
    "history": "^4.6.3",
    "html-webpack-plugin": "2.29.0",
    "idb-keyval": "^2.3.0",
    "js-beautify": "^1.6.14",
    "js-cookie": "^2.1.4",
    "jsbin-id": "^1.0.0",
    "json-lint": "^0.1.0",
    "jwt-decode": "^2.2.0",
    "less": "^2.7.2",
    "lodash.debounce": "^4.0.8",
    "loop-protect": "^2.1.6",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "postmate": "^1.1.9",
    "preact": "^8.2.7",
    "preact-compat": "^3.16.0",
    "prettier": "^1.5.3",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.2",
    "react-codemirror": "^1.0.0",
    "react-color": "^2.13.4",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.2",
    "react-error-overlay": "^1.0.9",
    "react-helmet": "^5.1.3",
    "react-hotkeys": "^0.10.0",
    "react-loadable": "^4.0.4",
    "react-notification": "^6.8.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-select": "^1.0.0-rc.5",
    "redux": "^3.7.2",
    "redux-pack": "^0.1.5",
    "redux-thunk": "^2.2.0",
    "sass.js": "^0.10.6",
    "source-map": "^0.6.1",
    "source-map-explorer": "^1.4.0",
    "strip-json-comments": "^2.0.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@remy/eslint": "^1.3.0",
    "babel-eslint": "^7.2.3",
    "cm-show-invisibles": "^2.0.2",
    "enzyme": "^2.9.1",
    "eslint": "^4.15.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.5.1",
    "expect": "^1.20.2",
    "jest": "^22.0.5",
    "node-sass": "^4.5.3",
    "node-sass-chokidar": "0.0.3",
    "react-addons-perf": "^15.4.2",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "worker-loader": "^0.8.1"
  },
  "engines": {
    "node": "8.x.x"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "REACT_APP_VERSION=$npm_package_version node scripts/start.js",
    "build": "npm run build-css && npm run build-app",
    "build-app": "REACT_APP_VERSION=$npm_package_version node scripts/build.js",
    "build-css": "node-sass-chokidar ./styles/ -o ./src/css/",
    "watch": "npm run build-css && node-sass-chokidar ./styles/ -o ./src/css/ --watch --recursive",
    "test": "node scripts/test.js --env=jsdom",
    "deploy": "npm run build && s3_website push",
    "alias-runner": "echo 'Running on localhost:'$(($(envy PORT)+1)); ssh -g -N -L $(($(envy PORT)+1)):localhost:$(envy PORT) localhost"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest-setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
