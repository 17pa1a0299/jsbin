{
  "name": "jsbin",
  "version": "5.0.0-alpha.29",
  "private": true,
  "main": "scripts/main.js",
  "files": [
    "scripts/main.js"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "printWidth": 80,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "dependencies": {
    "@remy/jsconsole": "^2.1.0",
    "@remy/react-splitter-layout": "^1.0.2",
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-jest": "^20.0.3",
